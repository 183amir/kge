	rm -rf $(OBJDIR_DEBUG)/Source
	rm -rf $(OBJDIR_DEBUG)/Source/gui

before_release: 
	test -d $(OBJDIR_RELEASE)/Source/math || mkdir -p $(OBJDIR_RELEASE)/Source/math
	test -d $(OBJDIR_RELEASE)/Source/ph || mkdir -p $(OBJDIR_RELEASE)/Source/ph
	test -d $(OBJDIR_RELEASE)/Source/io || mkdir -p $(OBJDIR_RELEASE)/Source/io
	test -d $(OBJDIR_RELEASE)/Source/sn || mkdir -p $(OBJDIR_RELEASE)/Source/sn
	test -d $(OBJDIR_RELEASE)/Source/ps || mkdir -p $(OBJDIR_RELEASE)/Source/ps
	test -d $(OBJDIR_RELEASE)/Source/core || mkdir -p $(OBJDIR_RELEASE)/Source/core
	test -d $(OBJDIR_RELEASE)/Source/gfx || mkdir -p $(OBJDIR_RELEASE)/Source/gfx
	test -d $(OBJDIR_RELEASE)/Source/efx || mkdir -p $(OBJDIR_RELEASE)/Source/efx
	test -d $(OBJDIR_RELEASE)/Source || mkdir -p $(OBJDIR_RELEASE)/Source
	test -d $(OBJDIR_RELEASE)/Source/gui || mkdir -p $(OBJDIR_RELEASE)/Source/gui

after_release: 

release: before_release out_release after_release

